{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "arrow-parens": false,
    "array-type": [
      false
    ],
    "curly": false,
    "eofline": false,
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "max-line-length": [
      true,
      140
    ],
    "member-ordering": [
      true,
      {
        "order": [
          "public-static-field",
          "private-static-field",
          "public-instance-field",
          "private-instance-field",
          "public-constructor",
          "private-constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "member-access": [
      false
    ],
    "no-console": {
      "severity": "warning"
    },
    "no-string-literal": false,
    "no-empty-interface": false,
    "no-empty": false,
    "no-unused-expression": false,
    "no-unused-variable": true,
    "object-literal-sort-keys": false,
    "one-line": [
      false
    ],
    "one-variable-per-declaration": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "variable-name": [
      false
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-type-operator",
      "check-preblock"
    ]
  },
  "rulesDirectory": []
}